@{
    Layout = "~/Views/_LayoutPage.cshtml";
}
@using Graduate_Council.Model;
  <div class="base"> 
   <div class="container"> 
    <div class="content"> 
     <div class="row"> 
      <div class="home-left"> 
       <div class="home-floor"> 
        <div id="myFocus"> 
         <div class="pic"> 
          <ul class="focus"> 
          @if (ViewData["bannerList"] != null)
         {
             foreach (BannerImg banner in (List<BannerImg>)ViewData["bannerList"])
             {                 
                <li><a href="@banner.Url"><img src="@banner.Path" thumb="新闻主题" text="@banner.Title"/></a></li>  
             }
         }
         else
         {
             <li><a href="javascript:;"><img src="" alt="无图片"/></a></li> 
         }    
          </ul> 
         </div> 
        </div> 
       </div> 
       <div class="home-floor"> 
        <div class="home-box"> 
         <div class="title" style="color:#D86B21;">
          <img src="/images/icon/new32.png" />信息前沿
         </div> 
         <table class="list-table"> 
          <tbody>
          @if ((List<NewInfo>)ViewData["list1"] != null)
          {
              foreach (NewInfo newInfo in (List<NewInfo>)ViewData["list1"])
              {
          <tr>
            <th><a href="/Home/DisplayNews?Id=@newInfo.Id&&Name=FrontNews">@newInfo.Title</a></th>
            <td>@newInfo.SubDateTime.ToShortDateString()</td>       
          </tr>
              }
          }
          else
          {
              <tr>
            <th>该分类没有新闻</th>                
          </tr> 
          }
          </tbody>
         </table> 
        </div> 
        <div class="home-box" style="margin-left:20px;"> 
         <div class="title" style="color:#5E9A33;">
          <img src="/images/icon/mphone32.png" />通知公告
         </div> 
         <table class="list-table"> 
          <tbody>
          @if ((List<NewInfo>)ViewData["list2"] != null)
          {
              foreach (NewInfo newInfo in (List<NewInfo>)ViewData["list2"])
              {
          <tr>
            <th><a href="/Home/DisplayNews?Id=@newInfo.Id&&Name=NoticeNews">@newInfo.Title</a></th>
            <td>@newInfo.SubDateTime.ToShortDateString()</td>       
          </tr>
              }
          }
          else { 
             
              <tr>
            <th>该分类没有新闻</th>                
          </tr> 
          
          }
          </tbody>
         </table> 
        </div> 
       </div> 
       <div class="home-floor"> 
        <div class="home-box"> 
         <div class="title" style="color:#2B6377;">
          <img src="/images/icon/pictures32.png" />校园油记
