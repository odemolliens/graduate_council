          }
          </tbody>
         </table> 
        </div> 
       </div> 
       <div class="home-floor"> 
        <div class="home-box"> 
         <div class="title" style="color:#3B8AD6;">
          <img src="/images/icon/info32.png" />分会动态
         </div> 
         <table class="list-table"> 
          <tbody>
          @if ((List<NewInfo>)ViewData["list5"] != null)
